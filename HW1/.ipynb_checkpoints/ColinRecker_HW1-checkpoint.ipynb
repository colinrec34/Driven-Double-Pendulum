    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c32cb75c",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Defining constants\n",
    "G = 6.67*10**-11 #m^3 kg^-1 s^-2\n",
    "M = 5.97*10**24 #kg\n",
    "R = 6371 #km"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b6c02a9e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def h(T):\n",
    "    return (G*M*T**2/(4*math.pi**2))**1/3 - R"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "0906d5fe",
   "metadata": {},
   "outputs": [],
   "source": [
    "T = float(input(\"Enter a value for T: \"))\n",
    "print(h(T), 'meters')"
   ]
  }
